let
  type Vector ~ record
    x: Integer,
    y: Integer
  end;
  var v: Vector;
  var result: Integer;
  func hola(x:Integer):Integer~x+x
in
  begin
    v.x := 3;
    v.y := 4;
    !result := v.norm()
    !result:= hi(result)
    result := hola(4);
    putint(result);
  end